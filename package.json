{
  "name": "broccoli-doc-tester",
  "version": "0.0.1",
  "description": "A small description of broccoli-doc-tester",
  "main": "dist/index.js",
  "scripts": {
    "build-i": "rm -rf dist && node --inspect-brk $(which broccoli) build dist",
    "build": "rm -rf dist && broccoli build dist",
    "build:serve": "broccoli serve",
    "test": "npm run build && mocha dist/tests --reporter tap",
    "test:lint": "npm run build && mocha dist/tests/lint.js",
    "test:debug": "npm run build && mocha debug dist/tests",
    "test:watch": "testem",
    "prepublish": "npm run build"
  },
  "keywords": [
    "broccoli",
    "plugin"
  ],
  "author": "",
  "license": "MIT",
  "repository": {},
  "files": [
    "dist/",
    "!dist/tests"
  ],
  "devDependencies": {
    "broccoli": "^2.3.0",
    "broccoli-babel-transpiler": "^7.2.0",
    "broccoli-cli": "^1.0.0",
    "broccoli-funnel": "^2.0.2",
    "broccoli-lint-eslint": "^5.0.0",
    "broccoli-merge-trees": "^3.0.2",
    "broccoli-stew": "^2.1.0",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "mocha-eslint": "^5.0.0",
    "regenerator-runtime": "^0.9.5",
    "testem": "^2.14.0"
  },
  "dependencies": {
    "broccoli-plugin": "^1.2.1",
    "doc-tester": "^1.0.1",
    "fs-extra": "^7.0.1",
    "fs-tree-diff": "^2.0.0",
    "glob": "^7.1.3",
    "heimdalljs": "^0.2.1",
    "heimdalljs-logger": "^0.1.7",
    "node-dir": "^0.1.17"
  }
}
